
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<title>@ViewBag.Title</title>


<!--CSS-->
    <link href="@Url.Content("~/Content/CSS/styles.css")" rel="stylesheet" type="text/css" />
    @*<link href="@Url.Content("~/Content/LoginWidget/styles/login-widget-main.css")" rel="stylesheet"
        type="text/css" />*@
<!--Google Webfont -->
    <link href='http://fonts.googleapis.com/css?family=Istok+Web' rel='stylesheet' type='text/css'>
    <script src="../../Scripts/JS/jquery-1.7.2.min.js" type="text/javascript"></script>
<!--Javascript-->
    @Scripts.Render("~/js/layout")
    <!--[if lt IE 9]>
        <script src="@Url.Content("~/Scripts/JS/html5.js")></script>
    <![endif]-->

    <script type="text/javascript" src="@Url.Content("~/Content/FancyBox/fancybox/jquery.fancybox-1.3.4.js")"></script>
    <link href="../../Content/FancyBox/fancybox/jquery.fancybox-1.3.4.css" rel="stylesheet"
        type="text/css" />
    <link href="../../Content/LoginBox/css/animate-custom.css" rel="stylesheet" type="text/css" />
    <link href="../../Content/LoginBox/css/style2.css" rel="stylesheet" type="text/css" />

    <style type="text/css">
        #fancybox-content
        {
            width:auto !important;
            border-width: 0px !important;         
        }
    </style>
    
<!-- mobile setting -->
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
</head>
<body>
<div class="wrapper">

    <div class="header_container">
        @{Html.RenderAction("_PartialHeader", "Layout");}
    </div>

    <div class="navigation_container">
        <!--Navigation Starts-->
        @{Html.RenderAction("_PartialNavigation","Layout",new {area=""});}
        <!--Navigation Ends-->
    </div>

    <div class="section_container">
        <!--Mid Section Starts-->

        <section>

            @RenderBody()
            
            <!--Newsletter_subscribe Starts-->
            @{Html.RenderAction("_PartialSubcribe", "Layout");}
            <!--Newsletter_subscribe Ends-->

        </section>
        <!--Mid Section Ends-->
        
    </div>

    <div class="footer_container">
         <!--Footer Starts-->
         @{Html.RenderAction("_PartialFooter", "Layout", new { area=""});}
         <!--Footer Ends-->

    </div>

</div>

</body>
</html>
